                            <table>
                                <tbody>
                                    <tr>
                                        <td>Diameter</td>
                                        <td>{{ neo.diameter }}</td>
                                    </tr>
                                    <tr>
                                        <td>Distance from Parent</td>
                                        <td>{{ neo.distanceFromParent }}</td>
                                    </tr>
                                    <tr>
                                        <td>Orbital Period</td>
                                        <td>{{ neo.orbitalPeriod }}</td>
                                    </tr>
                                    <tr>
                                        <td>Mass</td>
