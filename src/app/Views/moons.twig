{% for comet in comets %}
   <li class="accordion-item" data-accordion-item>
    <a class="accordion-title comet-name" style="color: yellow;">{{ comet.object_name }}</a>
    <div class="accordion-content" data-tab-content>
        <table>
            <tbody>
                <tr>
                    <td>Object</td>
                    <td>{{ comet.object }}</td>
                </tr>
                <tr>
                    <td>Epoch TDB</td>
                    <td>{{ comet.epoch_tdb }}</td>
                </tr>
                <tr>
                    <td>TP TDB</td>
                    <td>{{ comet.tp_tdb }}</td>
                </tr>
                <tr>
                    <td>Eccentricity</td>
                    <td>{{ comet.e }}</td>
                </tr>
                <tr>
                    <td>Inclination (deg)</td>
                    <td>{{ comet.i_deg }}</td>
                </tr>
                <tr>
                    <td>Argument of Perihelion (deg)</td>
                    <td>{{ comet.w_deg }}</td>
                </tr>
                <tr>
                    <td>Longitude of Ascending Node (deg)</td>
                    <td>{{ comet.node_deg }}</td>
                </tr>
                <tr>
                    <td>Perihelion Distance (AU)</td>
                    <td>{{ comet.q_au_1 }}</td>
                </tr>
                <tr>
                    <td>Aphelion Distance (AU)</td>
                    <td>{{ comet.q_au_2 }}</td>
                </tr>
                <tr>
                    <td>Orbital Period (yr)</td>
                    <td>{{ comet.p_yr }}</td>
                </tr>
                <tr>
                    <td>Minimum Orbital Intersection Distance (AU)</td>
                    <td>{{ comet.moid_au }}</td>
                </tr>
            </tbody>
        </table>
    </div>
</li>
{% endfor %}

