{% spaceless %}
    <div class="menu-title">Moons, <span style="color: blue;">Asteroids</span>, <span style="color: red;">Hazardous Asteroids</span>, <span style="color: yellow;">Comets</span></div>
    <ul class="accordion moon-list" data-accordion>
        {% for moon in moons %}
            <li class="accordion-item" data-accordion-item>
                {% set moonId = moon.id|default(loop.index) %}

                <a class="accordion-title moon-name" data-id="{{ moonId }}">{{ moon.name }}</a>
                <div class="accordion-content" data-tab-content>
                    <table>
                        <tbody>
                            <tr>
                                <td>Diameter</td>
                                <td>{{ moon.diameter }}</td>
                            </tr>
                            <tr>
                                <td>Semi-major Axis</td>
                                <td>{{ moon.distanceFromParent|number_format(0, ',') }} km</td>
                            </tr>
                            <tr>
                                <td>Orbital Period</td>
                                <td>{{ moon.orbitalPeriod }} days</td>
                            </tr>
                            <tr>
                                <td>Mass</td>
                                <td>{{ moon.mass }} kg</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </li>

            {% if moon.name == 'Moon' %}
                {% set neos = [
            {
                'id': '433',
